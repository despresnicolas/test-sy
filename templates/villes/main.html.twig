{% extends 'base.html.twig' %}

{% block body %}
    {% set array = [] %}
    {% for ville in villes %}
        {% set array = array|merge([{ville: ville.nom, code_postal: ville.codePostal}]) %}
    {% endfor %}

    {% set vars = {values: array, title: 'villes'} %}

    {% include 'includes/gestionTemplate.html.twig' with vars only %}
{% endblock %}

{% block title %}{% endblock %}


